      <option key="line" value="line" />
      <option key="switch" value="line 'switch On/Off'" />
      <option key="dimmer" value="line 'dimmer Up/Stop/Down'" />
      <option key="pourcent" value="line '%'" />
      <option key="area" value="area" />
      <option key="areaspline" value="areaspline" />   
      <option key="spline" value="spline" />
      <option key="temperature" value="spline 'temperature'" />       
		</setting>
    <setting label="" type="separator" />
    <setting id="id3" label="Object" type="object" />
    <setting id="typecurve3" label="Type of the object" type="list" default="spline">
      <option key="line" value="line" />
      <option key="switch" value="line 'switch On/Off'" />
      <option key="dimmer" value="line 'dimmer Up/Stop/Down'" />
      <option key="pourcent" value="line '%'" />
      <option key="area" value="area" />
      <option key="areaspline" value="areaspline" />   
      <option key="spline" value="spline" />
      <option key="temperature" value="spline 'temperature'" />       
		</setting>
    <setting label="" type="separator" />
    <setting id="id4" label="Object" type="object" />
    <setting id="typecurve4" label="Type of the object" type="list" default="spline">
      <option key="line" value="line" />
      <option key="switch" value="line 'switch On/Off'" />
      <option key="dimmer" value="line 'dimmer Up/Stop/Down'" />
      <option key="pourcent" value="line '%'" />
      <option key="area" value="area" />
      <option key="areaspline" value="areaspline" />   
